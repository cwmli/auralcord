{
  "name": "auralcord-client-server",
  "version": "0.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "build": "babel ./client -d build",
    "build:css": "tachyons ./client/src/css/App.css > ./build/public/index.css --minify",
    "build:watch": "babel ./client -d build --watch",
    "build:client": "webpack --config ./webpack.config.js/",
    "build:watch:client": "webpack --config ./webpack.config.js/ --watch",
    "build:prod": "npm run build && npm run build:client",
    "start": "npm run build:prod && NODE_ENV=production node ./build/server/server.js",
    "start:dev": "concurrently 'npm run build:watch' 'npm run build:watch:client' 'nodemon --exec babel-node ./build/server/server.js'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-particles-js": "^2.3.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "compression": "^1.7.3",
    "concurrently": "^3.5.0",
    "css-loader": "^1.0.1",
    "express": "^4.16.2",
    "file-loader": "^2.0.0",
    "nodemon": "^1.18.6",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "style-loader": "^0.23.1",
    "tachyons": "^4.11.1",
    "tachyons-cli": "^1.3.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
